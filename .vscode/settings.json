{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  },
  "stylelint.validate": [
    "scss",
    "css"
  ],
  "cSpell.words": [
    "commitlint",
    "lowcode",
    "nprogress",
    "pinia",
    "stylelint",
    "tailwindcss",
    "typecheck",
    "typeit",
    "vnode"
  ],
  "vue.codeActions.enabled": false
}